{
  "name": "deepdetect-js",
  "version": "1.8.0",
  "description": "DeepDetect JS client",
  "main": "lib/deepdetect.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-register --require babel-polyfill --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --require babel-polyfill --colors -w ./test/*.spec.js -w ./src/*.js",
    "test_pending": "mocha --require babel-register --require babel-polyfill --colors ./test/pending.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jolibrain/deepdetect-js.git"
  },
  "keywords": [
    "deepdetect",
    "deep-learning",
    "machine-learning",
    "rest-api",
    "client",
    "javascript",
    "request"
  ],
  "author": {
    "name": "Alexandre Girard",
    "email": "alex.girard@jolibrain.com",
    "url": "http://jolibrain.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jolibrain/deepdetect-js/issues"
  },
  "homepage": "https://deepdetect.com",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.6",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^4.0.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "braces": "^3.0.2",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.13.1"
  }
}
